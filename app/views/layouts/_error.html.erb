<% if obj.errors.any? %>
  <div id="error_explanation">
    <h3 class="text-danger"><%= obj.errors.count %> つの情報を正しく入力してください</h3>
    <ul>
      <% obj.errors.full_messages.each do |message| %>
        <li class="text-danger"><%= message %></li>
        </br>
      <% end %>
    </ul>
  </div>
<% end %>