<% provide(:title, "管理者ログイン") %>
<div class="container">
  <div class="row">
    <div class="col-xs-offset-1 col-xs-11">
      <h2 class="head-line title_h2 text-center">管理者ログイン</h2>
        <%= form_with model: @admin, url: admin_session_path, local: true do |f| %>
        <%= render "session", f: f %>
      <div class="actions col-xs-6">
        <%= f.submit "　ログイン　", class: "btn btn-secondary" %>
      </div>
      <% end %>
    </div>
  </div>
</div>